  "license": "MIT",
  "dependencies": {
    "@google-cloud/secret-manager": "^4.1.4",
    "graphql-request": "^5.0.0",
    "near-api-js": "^0.44.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.13.7",
    "@graphql-codegen/client-preset": "1.1.1",
    "@graphql-codegen/introspection": "2.2.1"
  }
}